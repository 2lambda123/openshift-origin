{
  "kind": "SecurityContextConstraints",
  "apiVersion":"v1beta3",

  "metadata": {
    "name": "scc-serviceaccounts",
    "labels": {
      "name": "scc-serviceaccounts"
    }
  },
  "runAsUser": {
    "type": "MustRunAsRange"
  },
  "seLinuxContext": {
    "type": "MustRunAs"
  },
  "groups": ["system:serviceaccounts", "system:authenticated"]
}
