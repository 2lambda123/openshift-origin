{
  "kind": "List",
  "apiVersion": "v1beta3",
  "metadata": {},
  "items": [
    {
      "apiVersion": "v1beta1",
      "kind": "ImageStream",
      "metadata": {
        "name": "ruby-20-centos-buildcli"
      },
      "spec": {
        "dockerImageRepository": "openshift/ruby-20-centos",
        "tags": [
          {
            "name": "valid",
            "dockerImageReference": "openshift/ruby-20-centos7:success"
          }
        ]
      }
    },
    {
      "apiVersion": "v1beta1",
      "kind": "BuildConfig",
      "labels": {
        "name": "ruby-sample-build"
      },
      "metadata": {
        "name": "ruby-sample-build-validtag"
      },
      "parameters": {
        "output": {
          "to": {
            "name": "origin-ruby-sample"
          }
        },
        "source": {
          "git": {
            "uri": "git://github.com/openshift/ruby-hello-world.git"
          },
          "type": "Git"
        },
        "strategy": {
          "stiStrategy": {
            "from": {
              "name": "ruby-20-centos-buildcli"
            },
            "tag": "valid",
            "scripts": "https://raw.githubusercontent.com/openshift/ruby-20-centos/master/.sti/bin"
          },
          "type": "STI"
        }
      },
      "triggers": [
        {
          "imageChange": {
            "from": {
              "name": "ruby-20-centos-buildcli"
            },
            "image": "openshift/ruby-20-centos",
            "tag": "valid"
          },
          "type": "imageChange"
        }
      ]
    },
    {
      "apiVersion": "v1beta1",
      "kind": "BuildConfig",
      "labels": {
        "name": "ruby-sample-build"
      },
      "metadata": {
        "name": "ruby-sample-build-invalidtag"
      },
      "parameters": {
        "output": {
          "to": {
            "name": "origin-ruby-sample"
          }
        },
        "source": {
          "git": {
            "uri": "git://github.com/openshift/ruby-hello-world.git"
          },
          "type": "Git"
        },
        "strategy": {
          "stiStrategy": {
            "from": {
              "name": "ruby-20-centos-buildcli"
            },
            "tag": "invalid",
            "scripts": "https://raw.githubusercontent.com/openshift/ruby-20-centos/master/.sti/bin"
          },
          "type": "STI"
        }
      },
      "triggers": [
        {
          "imageChange": {
            "from": {
              "name": "ruby-20-centos-buildcli"
            },
            "image": "openshift/ruby-20-centos",
            "tag": "invalid"
          },
          "type": "imageChange"
        }
      ]
    }
  ]
}