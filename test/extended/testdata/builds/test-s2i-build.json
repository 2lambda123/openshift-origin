{
  "kind": "BuildConfig",
  "apiVersion": "build.openshift.io/v1",
  "metadata": {
    "name": "test",
    "labels": {
      "name": "test"
    }
  },
  "spec": {
    "triggers": [],
    "source": {
      "type": "Git",
      "git": {
        "uri":"https://github.com/sclorg/nodejs-ex"
      }
    },
    "strategy": {
      "type": "Source",
      "sourceStrategy": {
        "env": [
          {
            "name": "BUILD_LOGLEVEL",
            "value": "5"
          }
        ],
        "from": {
          "kind": "DockerImage",
          "name": "quay.io/redhat-developer/test-build-simples2i:1.2"
        }
      }
    },
    "output": {
      "to": {
        "kind": "ImageStreamTag",
        "name": "test:latest"
      },
      "imageLabels": [
        {
          "name": "user-specified-label",
          "value": "arbitrary-value"
        },
        {
          "name": "io.k8s.display-name",
          "value": "overridden"
        },
        {
          "name": "io.openshift.builder-version",
          "value": "overridden2"
        }
      ]
    }
  }
}
